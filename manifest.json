{
    "name": "phishBuster",
    "version": "1.0",
    "description": "Protects the user from visiting potential phishing sites!",
    "manifest_version": 2,
    "background": {
      "scripts": [
	  "top-sites.js",
	  "Main.js"
      ],
      "persistent": false
    },
	"content_scripts": [
      {
     "matches": ["<all_urls>"],
     "js": ["doc.js","psl.js"]
      }
    ],

    "browser_action": {
      "default_popup": "popup.html"
    },
    "permissions": ["<all_urls>"]
 }
